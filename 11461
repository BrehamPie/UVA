#include<stdio.h>
#include<math.h>
int main()
{
    int i,n,p,q,c=0;
    double s;
    while(1)
    {
        scanf("%d %d",&p,&q);
        if(p==0 && q==0)
            return 0;
        for(i=p;i<=q;i++)
        {
            s=sqrt(i);
            n=sqrt(i);
            if (s-n==0)
                c++;
        }
    printf("%d\n",c);
    c=0;}
    return 0;
}
