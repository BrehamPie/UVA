#include<bits/stdc++.h>
using namespace std;
#define rep(i,n) for(int i=0;i<n;i++)
int main()
{
    int t;cin>>t;
    while(t--)
    {
        int sum=0,n;cin>>n;
        while(n--)
        {
            int a,b,c;
            cin>>a>>b>>c;sum+=(a*c);
        }
        cout<<sum<<endl;
    }
    return 0;
}
