#include<bits/stdc++.h>
#define rep(i,n) for(int i=0;i<n;i++)
#define per(i,n) for(int i=n-1;i>=0;i--)
#define pb push_back
#define mp make_pair
#define MOD 1000000007
using namespace std;

int main()
{
    char c,cnt=0;
    while(c=getchar())
    {
        if(c==EOF) break;
        if(c=='"')
        {
            cnt++;
            if(cnt%2)cout<<"``";
            else cout<<"''";
        }
        else cout<<c;
    }
}
