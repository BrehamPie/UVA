#include<stdio.h>
#include<math.h>
int main()
{
    int k=1,a,b,sum=0,n,i;
    scanf("%d",&n);
    while(n--)
    {
        scanf("%d %d",&a,&b);
        for(i=a;i<=b;i++)
            if(i%2!=0)
                sum+=i;
        printf("Case %d: %d\n",k,sum);
        sum=0;
        k++;
    }
    return 0;
}
