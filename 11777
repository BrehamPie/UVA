#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t,i=1;cin>>t;
    while(t--)
    {
        int a,b,c,d,e,f,g;
        cin>>a>>b>>c>>d>>e>>f>>g;
        int avg=a+b+c+d+((e+f+g-min(min(e,f),min(f,g)))/2);
        printf("Case %d: ",i++);
        if(avg>89) cout<<"A\n";
        else if(avg>79) cout<<"B\n";
        else if(avg>69) cout<<"C\n";
        else if(avg>59) cout<<"D\n";
        else cout<<"F\n";

    }
    return 0;
}
